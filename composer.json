{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "3.0.4",
        "composer/package-versions-deprecated": "1.11.99.3",
        "doctrine/annotations": "1.13.3",
        "doctrine/doctrine-bundle": "2.7.1",
        "doctrine/doctrine-migrations-bundle": "3.2.2",
        "doctrine/orm": "2.13.4",
        "nelmio/cors-bundle": "2.2.0",
        "phpdocumentor/reflection-docblock": "5.3.0",
        "symfony/apache-pack": "1.0.1",
        "symfony/asset": "6.4.8",
        "symfony/console": "6.4.12",
        "symfony/debug-bundle": "6.4.8",
        "symfony/dotenv": "6.4.12",
        "symfony/expression-language": "6.4.11",
        "symfony/flex": "2.4.7",
        "symfony/framework-bundle": "6.4.12",
        "symfony/http-client": "6.4.12",
        "symfony/intl": "6.4.12",
        "symfony/monolog-bundle": "3.10.0",
        "symfony/property-access": "6.4.11",
        "symfony/property-info": "6.4.10",
        "symfony/proxy-manager-bridge": "6.4.8",
        "symfony/runtime": "6.4.12",
        "symfony/security-bundle": "6.4.11",
        "symfony/serializer": "6.4.12",
        "symfony/translation": "6.4.12",
        "symfony/twig-bundle": "6.4.12",
        "symfony/validator": "6.4.12",
        "symfony/var-dumper": "6.4.11",
        "symfony/webpack-encore-bundle": "1.17.2",
        "symfony/yaml": "6.4.12",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "phpstan": "vendor/bin/phpstan -c phpstan.neon",
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    },
    "require-dev": {
        "doctrine/coding-standard": "10.0.0",
        "phpstan/extension-installer": "1.2.0",
        "phpstan/phpstan": "1.9.2",
        "phpstan/phpstan-deprecation-rules": "1.0.0",
        "phpstan/phpstan-doctrine": "1.3.23",
        "phpstan/phpstan-strict-rules": "1.4.4",
        "phpstan/phpstan-symfony": "1.2.16",
        "squizlabs/php_codesniffer": "3.7.1",
        "symfony/stopwatch": "6.4.8",
        "symfony/web-profiler-bundle": "6.4.11"
    }
}
